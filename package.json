{
  "name": "mypli2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:web": "pnpm --filter ./apps/web dev",
    "dev:api": "pnpm --filter ./apps/api dev",
    "dev:all": "concurrently -r \"pnpm dev:api\" \"pnpm dev:web\"",
    "build:web": "pnpm --filter ./apps/web build",
    "build:api": "pnpm --filter ./apps/api build",
    "build:all": "pnpm build:api && pnpm build:web",
    "docker:up": "docker compose -f ./infra/docker-compose.dev.yaml --env-file ./infra/.env.dev up -d --build",
    "docker:down": "docker compose -f ./infra/docker-compose.dev.yaml down",
    "docker:logs": "docker compose -f ./infra/docker-compose.dev.yaml logs -f api web"
  },
